@{
    Layout = null;
}


@foreach (var w in ViewData["wushui"] as IEnumerable<Wushui>)
{
    <li class="w-listli wushui">

        <a href="#">
            <h3 class="wlisth3">
                @w.id_name
                <!--<i class="wlistcare">有任务</i>-->
            </h3>
            <!-- 一号回流泵 -->
            @if (w.hlb1_fault == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>一号回流泵</font><br />

                </p>
            }

            <!-- 一号提升泵 -->
            @if (w.tsb1_fault == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>一号提升泵</font><br />

                </p>
            }

            <!-- 一号污泥泵 -->
            @if (w.wn1_fault == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>一号污泥泵</font><br />

                </p>
            }

            <!-- 一号风机 -->
            @if (w.fan1_fault == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>一号风机</font><br />

                </p>
            }

            <!-- 二号回流泵 -->
            @if (w.hlb2_fault == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>二号回流泵</font><br />

                </p>
            }

            <!-- 二号提升泵 -->
            @if (w.tsb2_fault == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>二号提升泵</font><br />

                </p>
            }

            <!-- 调节池低液位报警S01 -->
            @if (w.tjc_low == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>调节池液位低</font><br />

                </p>
            }

            <!-- 调节池高液位报警S01 -->
            @if (w.tjc_high == 1)
            {
                <p class="wlistp">
                    <font style="color:#ff0000"><b>故障：</b>调节池液位高</font><br />

                </p>
            }



        </a>


    </li>

}
